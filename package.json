{
  "name": "github-user-search",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "lint": "eslint src --ext ts,tsx",
    "lint:fix": "yarn lint --fix",
    "prepare": "husky install",
    "test": "jest",
    "test:watch": "yarn test --watch",
    "test:coverage": "yarn test --coverage",
    "teste2e": "mocha --timeout 60000 --require mocha-steps automation-test/testCases/E2E --reporter mochawesome",
    "integrationtest": "mocha --timeout 60000 --require mocha-steps automation-test/testCases/integration --reporter mochawesome",
    "unittest": "jest automation-test/testCases/unitTest"
  },
  "dependencies": {
    "axios": "^0.26.1",
    "classnames": "^2.3.1",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "react-router-dom": "^6.3.0",
    "sass": "^1.50.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^16.2.3",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.1.1",
    "@types/jest": "^27.4.1",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "@vitejs/plugin-react": "^1.3.0",
    "chai": "^4.2.0",
    "chromedriver": "^123.0.0",
    "cz-emoji-conventional": "^1.0.1",
    "eslint": "^8.14.0",
    "eslint-config-airbnb": "^18.1.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-react-app": "^7.0.1",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-react": "^7.29.4",
    "eslint-plugin-react-hooks": "^4.4.0",
    "husky": "^7.0.4",
    "identity-obj-proxy": "^3.0.0",
    "jest": "27.5.1",
    "jest-runner-eslint": "^1.0.0",
    "lint-staged": ">=10",
    "mocha": "^10.3.0",
    "mocha-steps": "^1.3.0",
    "mochawesome": "^7.1.3",
    "node-fetch": "^2.7.0",
    "prettier": "^2.6.2",
    "selenium-webdriver": "^4.18.1",
    "ts-jest": "^27.1.4",
    "ts-node": "^10.7.0",
    "typescript": "^4.6.3",
    "vite": "^2.9.5",
    "vite-tsconfig-paths": "^3.4.1"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-emoji-conventional"
    }
  },
  "lint-staged": {
    "*.tsx,*.ts": "eslint --cache --fix"
  }
}
